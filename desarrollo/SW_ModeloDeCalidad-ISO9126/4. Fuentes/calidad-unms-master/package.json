{
  "name": "adm-ecommerce-vg",
  "version": "0.0.0",
  "scripts": {
    "check:node": "check-node-version --package",
    "build": "node --max_old_space_size=5048 && npm run prebuild & ng build --configuration=local",
    "build:dev": "node --max_old_space_size=5048 && npm run prebuild && ng build --configuration=develop",
    "build:prod": "node --max_old_space_size=5048 && npm run prebuild && ng build --configuration=production",
    "prebuild": "node git-version.js",
    "start": "npm run prebuild & ng serve --configuration=local --port=5200 --open",
    "start:dev": "npm run prebuild & ng serve --configuration=develop --port=5200 --open",
    "start:prod": "npm run prebuild & ng serve --configuration=production --port=5200 --open",
    "tslint": "gulp tslint --errorsAllowed 0 --reportLimit 100",
    "tslint:fix": "tslint -c tslint.json --fix src/**/*.ts -t verbose",
    "tslint:report": "node scripts/tasks.js"
  },
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=12.0 <13.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run tslint",
      "pre-push": "npm run build:prod"
    }
  },
  "keywords": [
    "Node",
    "Gulp",
    "Javascript",
    "Angular",
    "Ecommerce",
    "Video Games"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/custom"
  },
  "dependencies": {
    "@angular/animations": "^9.1.6",
    "@angular/cdk": "^9.2.3",
    "@angular/common": "^9.1.6",
    "@angular/compiler": "^9.1.6",
    "@angular/core": "^9.1.6",
    "@angular/forms": "^9.1.6",
    "@angular/platform-browser": "^9.1.6",
    "@angular/platform-browser-dynamic": "^9.1.6",
    "@angular/router": "^9.1.6",
    "@danielmoncada/angular-datetime-picker": "^9.3.0",
    "@fortawesome/angular-fontawesome": "^0.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@ngrx/effects": "^9.1.2",
    "@ngrx/store": "^9.1.2",
    "@ngrx/store-devtools": "^9.1.2",
    "bootstrap": "^4.5.2",
    "jquery": "^3.5.1",
    "lodash.template": "^4.5.0",
    "material-dashboard": "^2.1.0",
    "material-design-icons": "^3.0.1",
    "moment": "2.25.3",
    "ngx-color-picker": "^9.1.0",
    "popper.js": "^1.16.1",
    "rxjs": "^6.5.5",
    "simplebar": "^5.2.0",
    "swagger-angular-generator": "^4.2.2",
    "swagger-ui-express": "^4.1.4",
    "tslib": "^1.11.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.5",
    "@angular/cli": "~9.1.5",
    "@angular/compiler-cli": "^9.1.6",
    "@angular/language-service": "^9.1.6",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~13.13.5",
    "angular-tslint-rules": "^1.20.4",
    "ansi-colors": "^4.1.1",
    "check-node-version": "^4.0.3",
    "codelyzer": "^5.2.2",
    "fancy-log": "^1.3.3",
    "git-describe": "^4.0.4",
    "gulp": "^4.0.2",
    "gulp-plumber": "^1.2.1",
    "gulp-tslint": "^8.1.4",
    "handlebars": "^4.7.6",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.0.5",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "protractor": "^7.0.0",
    "through": "^2.3.8",
    "ts-node": "~8.10.1",
    "tslint": "^6.1.3",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-html-report": "^2.0.3",
    "typescript": "~3.8.3",
    "yargs": "^15.3.1"
  }
}
